{
  "name": "chessmate",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "ios:build:clean": "rm -rf ios && npx expo prebuild --clean && cd ios && pod install --repo-update && cd .. && npx expo run:ios --no-build-cache",
    "ios:metro": "npx expo start -c --dev-client",
    "ios:dev": "./scripts/ios-dev.sh",
    "dev:ios-remote": "cross-env EXPO_USE_DEV_SERVER=1 EXPO_NO_TELEMETRY=1 cross-env NODE_OPTIONS=--max_old_space_size=4096 expo start --tunnel --clear",
    "build:ios:sim": "eas build --platform ios --profile dev-sim --non-interactive",
    "build:ios:list": "eas build:list --platform ios --status finished --limit 10"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/ui": "0.2.0-beta.6",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/elements": "^2.6.4",
    "@sentry/react-native": "^7.1.0",
    "@shopify/flash-list": "^2.0.2",
    "@shopify/react-native-skia": "^2.2.12",
    "@supabase/supabase-js": "^2.57.4",
    "@uzigogetta/react-native-stockfish-jsi": "workspace:*",
    "chess.js": "^1.4.0",
    "expo": "~54.0.12",
    "expo-audio": "~1.0.13",
    "expo-blur": "~15.0.7",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "^6.0.13",
    "expo-file-system": "~19.0.16",
    "expo-font": "~14.0.8",
    "expo-glass-effect": "~0.1.4",
    "expo-haptics": "~15.0.7",
    "expo-insights": "~0.10.7",
    "expo-linear-gradient": "15.0.7",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.10",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "immer": "^10.1.3",
    "nanoid": "^5.1.5",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-keyboard-controller": "^1.18.5",
    "react-native-mmkv": "^3.3.1",
    "react-native-reanimated": "~4.1.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-worklets": "0.5.1",
    "react-native-worklets-core": "^1.6.2",
    "socket.io-client": "^4.8.1",
    "stockfish": "^17.1.0",
    "zod": "^4.1.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.81.1",
    "@sentry/cli": "^2.53.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest-expo": "^54.0.12",
    "msw": "^2.11.2",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2",
    "cross-env": "^7.0.3"
  },
  "private": true,
  "pnpm": {
    "publicHoistPattern": [
      "*"
    ]
  }
}
