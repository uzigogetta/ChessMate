{
  "expo": {
    "name": "ChessMate",
    "slug": "chessmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "chessmate",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uzigogetta.chessmate",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.us.chessmate",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": true
    },
    "updates": {
      "url": "https://u.expo.dev/b9979c02-b8b3-4056-9f3b-98ad07ab46f8",
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      "expo-router",
      "./plugins/withPredictiveBack"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b9979c02-b8b3-4056-9f3b-98ad07ab46f8"
      },
      "serverUrl": "$(EXPO_PUBLIC_SERVER_URL)"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
